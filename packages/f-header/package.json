  "license": "Apache-2.0",
  "engines": {
    "node": ">=10.0.0"
  },
  "keywords": [
    "fozzie"
  ],
  "scripts": {
    "prepublishOnly": "yarn && yarn lint && yarn test && yarn build",
    "build": "vue-cli-service build --target lib --name f-header ./src/index.js",
    "demo": "vue serve --port 8080 ./src/demo/Index.vue",
    "lint": "vue-cli-service lint --no-fix",
    "lint:fix": "yarn lint --fix",
    "test": "vue-cli-service test:unit",
    "test-component:chrome": "run-p --race demo webdriver:delay:chrome",
    "test:wait-for-server": "node ../../test/infrastructure/healthcheck.js 8080",
    "webdriver:delay:chrome": "yarn test:wait-for-server && yarn webdriver:start:chrome",
    "webdriver:start:chrome": "wdio ../../wdio.conf.js"
  },
  "scriptsInfo": {
    "test-component:chrome": "Runs the functional test suite against Chrome.",
    "webdriver:delay:chrome": "Waits for Vue to start before running the Chrome webdriver to prevent a race condition. Not for direct use.",
    "webdriver:start:chrome": "Runs the Chrome webdriver. Not for direct use."
  },
  "browserslist": [
    "extends @justeat/browserslist-config-fozzie"
  ],
  "dependencies": {
    "@justeat/f-services": "0.13.0",
    "@justeat/f-vue-icons": "1.0.0-beta.11",
    "axios": "0.19.2"
  },
  "peerDependencies": {
    "@justeat/browserslist-config-fozzie": ">=1.1.1",
    "@justeat/f-trak": ">=0.6.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "4.4.6",
    "@vue/cli-plugin-eslint": "3.9.2",
    "@vue/cli-plugin-unit-jest": "4.4.6",
    "@vue/test-utils": "1.0.3",
    "node-sass-magic-importer": "5.3.2"
  }
}
