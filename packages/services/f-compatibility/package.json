{
  "name": "@justeat/f-compatibility",
  "description": "Fozzie Compatibility - Service to check browser compatibilities and warn users with incompatible browsers",
  "version": "0.2.1",
  "maxBundleSize": "10kB",
  "main": "dist/f-compatibility.umd.js",
  "module": "dist/f-compatibility.es.js",
  "source": "src/index.js",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/justeat/fozzie-components/tree/master/packages/services/f-compatibility",
  "contributors": [
    "Github contributors <https://github.com/justeat/fozzie-components/graphs/contributors>"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:justeat/fozzie-components.git"
  },
  "bugs": {
    "url": "https://github.com/justeat/fozzie-components/issues"
  },
  "license": "Apache-2.0",
  "engines": {
    "node": "^12 || ^14"
  },
  "keywords": [
    "fozzie"
  ],
  "scripts": {
    "prepublishOnly": "yarn lint && yarn test && yarn build",
    "build": "vite build",    
    "lint": "eslint \"!(dist)/**/*.{js,vue}\"",
    "lint:fix": "yarn lint --fix",
    "test": "jest",
    "ci:test:services": "yarn test",
    "test:coverage": "jest test:unit --coverage"
  },
  "browserslist": [
    "extends @justeat/browserslist-config-fozzie"
  ],
  "devDependencies": {
    "core-js": "3.22.5",
    "eslint": "7.32.0",
    "jest-extended": "0.11.5",
    "vite": "2.9.9",
    "bowser": "2.11.0"
  }
}
