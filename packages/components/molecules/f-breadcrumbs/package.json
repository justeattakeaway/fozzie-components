  "version": "3.1.0",
  "main": "dist/f-breadcrumbs.umd.min.js",
  "maxBundleSize": "5kB",
  "files": [
    "dist",
    "test-utils"
  ],
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/justeat/fozzie-components/tree/master/packages/components/molecules/f-breadcrumbs",
