  "scripts": {
    "prepublishOnly": "yarn lint && yarn test && yarn build",
    "build": "vue-cli-service build --target lib --name f-breadcrumbs src/index.js",
    "lint": "eslint \"!(dist)/**/*.{js,vue}\"",
    "lint:fix": "yarn lint --fix",
    "lint:style": "vue-cli-service lint:style",
