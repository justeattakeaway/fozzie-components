{
  "name": "@justeat/f-spinner",
  "description": "Fozzie Spinner - loading indicator",
  "version": "0.3.0",
  "main": "dist/f-spinner.umd.min.js",
  "maxBundleSize": "5kB",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/justeat/fozzie-components/tree/master/packages/components/atoms/f-spinner",
  "contributors": [
    "Github contributors <https://github.com/justeat/fozzie-components/graphs/contributors>"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:justeat/fozzie-components.git"
  },
  "bugs": {
    "url": "https://github.com/justeat/fozzie-components/issues"
  },
  "license": "Apache-2.0",
  "engines": {
    "node": ">=12.0.0"
  },
  "keywords": [
    "fozzie"
  ],
  "scripts": {
    "prepublishOnly": "yarn lint && yarn test && yarn build",
    "build": "vue-cli-service build --target lib --name f-spinner ./src/index.js",
    "lint": "eslint \"!(dist)/**/*.{js,vue}\"",
    "ci:build:atoms": "yarn build",
    "ci:lint:atoms": "yarn lint",
    "lint:fix": "yarn lint --fix",
    "lint:style": "vue-cli-service lint:style",
    "test": "vue-cli-service test:unit",
    "test:watch": "vue-cli-service test:unit --watch",
    "ci:test:atoms": "yarn test",
    "test-component:chrome": "cross-env-shell TEST_TYPE=component wdio ../../../../wdio-chrome.conf.js",
    "test-a11y:chrome": "cross-env-shell TEST_TYPE=a11y wdio ../../../../wdio-chrome.conf.js",
    "test-a11y:chrome:atoms": "cross-env-shell TEST_TYPE=a11y wdio ../../../../wdio-chrome.conf.js",
    "ci:test-component:chrome:atoms": "cross-env-shell TEST_TYPE=component wdio ../../../../wdio-chrome.conf.js"
  },
  "browserslist": [
    "extends @justeat/browserslist-config-fozzie"
  ],
  "dependencies": {
    "@justeat/f-services": "1.7.0"
  },
  "peerDependencies": {
    "@justeat/browserslist-config-fozzie": ">=1.2.0"
  },
  "devDependencies": {
    "@justeat/f-wdio-utils": "0.5.0",
    "@samhammer/vue-cli-plugin-stylelint": "2.0.1",
    "@vue/cli-plugin-babel": "4.4.6",
    "@vue/cli-plugin-unit-jest": "4.4.6",
    "@vue/test-utils": "1.0.3"
  }
}
