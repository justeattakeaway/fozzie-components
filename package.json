  },
  "stylelint": {
    "extends": "@justeat/stylelint-config-fozzie"
  },
  "workspaces": [
    "packages/*",
    "packages/components/atoms/*",
    "packages/components/molecules/*",
    "packages/components/organisms/*",
    "packages/components/templates/*",
    "packages/components/pages/*",
    "packages/services/*",
    "packages/tools/*"
  ],
