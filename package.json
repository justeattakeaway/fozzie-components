    "axe-core": "4.4.2",
    "axe-reports": "1.1.11",
    "axios": "0.27.2",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "26.6.3",
    "babel-loader": "8.2.5",
    "bundlewatch": "0.3.3",
    "chromedriver": "96.0.0",
    "cross-env": "7.0.3",
    "css-loader": "1.0.1",
    "danger": "10.9.0",
    "eslint": "8.17.0",
    "eslint-import-resolver-webpack": "0.13.2",
