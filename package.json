{
  "name": "root",
  "version": "7.10.0",
  "private": true,
  "scripts": {
    "build": "turbo run build --continue",
    "bundlewatch": "bundlewatch --config bundlewatch.config.js",
    "lint": "turbo run lint --continue",
    "lint:fix": "turbo run lint -- --fix --continue",
    "lint:style": "turbo run lint:style --continue",
    "prepublishOnly": "turbo run prepublishOnly --continue",
    "release": "turbo publish --continue",
    "storybook:build": "turbo run storybook:build --continue",
    "storybook:build-changed": "cross-env-shell CHANGED_ONLY=true turbo run storybook:build --include-dependencies --continue",
    "storybook:serve": "turbo run storybook:serve --continue",
    "storybook:serve-changed": "cross-env-shell CHANGED_ONLY=true turbo run storybook:serve --continue",
    "storybook:serve-static": "turbo run storybook:serve-static --continue",
    "storybook:deploy": "turbo run storybook:deploy --continue",
    "test": "turbo run test --continue",
    "ci:test:tools": "turbo run ci:test:tools --continue",
    "ci:test:services": "turbo run ci:test:services --continue",
    "ci:test:atoms": "turbo run ci:test:atoms --continue",
    "ci:test:molecules": "turbo run ci:test:molecules --continue",
    "ci:test:organisms": "turbo run ci:test:organisms --continue",
    "ci:test:pages": "turbo run ci:test:pages --continue",
    "ci:test:templates": "turbo run ci:test:templates --continue",
    "ci:test-component:chrome:atoms": "cross-env-shell TEST_TYPE=component turbo run ci:test-component:chrome:atoms --concurrency=1 --continue",
    "ci:test:visual:atoms": "cross-env-shell TEST_TYPE=visual turbo run ci:test:visual:atoms --concurrency=1 --continue",
    "ci:test-a11y:chrome:atoms": "cross-env-shell TEST_TYPE=a11y turbo run ci:test-a11y:chrome:atoms --concurrency=1 --continue",
    "ci:test-component:chrome:molecules": "cross-env-shell TEST_TYPE=component turbo run ci:test-component:chrome:molecules --concurrency=1 --continue",
    "ci:test:visual:molecules": "cross-env-shell TEST_TYPE=visual turbo run ci:test:visual:molecules --concurrency=1 --continue",
    "ci:test-a11y:chrome:molecules": "cross-env-shell TEST_TYPE=a11y turbo run ci:test-a11y:chrome:molecules --concurrency=1 --continue",
    "ci:test-component:chrome:organisms": "cross-env-shell TEST_TYPE=component turbo run ci:test-component:chrome:organisms --concurrency=1 --continue",
    "ci:test:visual:organisms": "cross-env-shell TEST_TYPE=visual turbo run ci:test:visual:organisms --concurrency=1 --continue",
    "ci:test-a11y:chrome:organisms": "cross-env-shell TEST_TYPE=a11y turbo run ci:test-a11y:chrome:organisms --concurrency=1 --continue",
    "ci:test-component:chrome:pages": "cross-env-shell TEST_TYPE=component turbo run ci:test-component:chrome:pages --concurrency=1 --continue",
    "ci:test:visual:pages": "cross-env-shell TEST_TYPE=visual turbo run ci:test:visual:pages --concurrency=1 --continue",
    "ci:test-a11y:chrome:pages": "cross-env-shell TEST_TYPE=a11y turbo run ci:test-a11y:chrome:pages --concurrency=1 --continue",
    "test-component:chrome": "cross-env-shell TEST_TYPE=component turbo run test-component:chrome --concurrency=1 --continue",
    "test:visual": "cross-env-shell TEST_TYPE=visual turbo run test:visual --concurrency=1 --continue",
    "test-a11y:chrome": "cross-env-shell TEST_TYPE=a11y turbo run test-a11y:chrome --concurrency=1 --continue",
    "test:consumer": "turbo run test:consumer",
    "test:wait-for-server": "node ./test/infrastructure/healthcheck.js",
    "allure:generate": "allure generate ./test/results/allure-results --clean && yarn allure:clean",
    "allure:open": "yarn allure:generate && yarn allure open",
    "allure:clean": "rimraf test/results/allure-results",
    "prepare": "husky install"
  },
  "resolutions": {
    "webpack": "4.46.0"
  },
  "devDependencies": {
    "@babel/cli": "7.17.10",
    "@babel/eslint-parser": "7.18.2",
