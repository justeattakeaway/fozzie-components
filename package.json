{
  "name": "root",
  "version": "7.7.0",
  "private": true,
  "scripts": {
    "build": "cross-env-shell \"turbo run build $LERNA_ARGS --stream\"",
    "bundlewatch": "cross-env-shell \"bundlewatch --config bundlewatch.config.js\"",
    "lint": "cross-env-shell \"turbo run lint $LERNA_ARGS --stream\"",
    "lint:fix": "turbo run lint -- --fix",
    "lint:style": "turbo run lint:style --stream",
    "storybook:build": "turbo run storybook:build --stream",
    "storybook:build-changed": "cross-env-shell CHANGED_ONLY=true turbo run storybook:build $LERNA_ARGS --include-dependencies --stream",
    "storybook:serve": "turbo run storybook:serve --stream",
    "storybook:serve-changed": "cross-env-shell CHANGED_ONLY=true turbo run storybook:serve --stream",
    "storybook:serve-static": "turbo run storybook:serve-static --stream",
    "storybook:deploy": "turbo run storybook:deploy --stream",
    "test": "cross-env-shell \"turbo run test $LERNA_ARGS --stream\"",
    "ci:test:tools": "cross-env-shell \"turbo run ci:test:tools $LERNA_ARGS --stream\"",
    "ci:test:services": "cross-env-shell \"turbo run ci:test:services $LERNA_ARGS --stream\"",
    "ci:test:atoms": "cross-env-shell \"turbo run ci:test:atoms $LERNA_ARGS --stream\"",
    "ci:test:molecules": "cross-env-shell \"turbo run ci:test:molecules $LERNA_ARGS --stream\"",
    "ci:test:organisms": "cross-env-shell \"turbo run ci:test:organisms $LERNA_ARGS --stream\"",
    "ci:test:pages": "cross-env-shell \"turbo run ci:test:pages $LERNA_ARGS --stream\"",
    "ci:test:templates": "cross-env-shell \"turbo run ci:test:templates $LERNA_ARGS --stream\"",
    "ci:test-component:chrome:atoms": "cross-env-shell TEST_TYPE=component turbo run ci:test-component:chrome:atoms $LERNA_ARGS --concurrency=1",
    "ci:test:visual:atoms": "cross-env-shell TEST_TYPE=visual turbo run ci:test:visual:atoms $LERNA_ARGS --concurrency=1",
    "ci:test-a11y:chrome:atoms": "cross-env-shell TEST_TYPE=a11y turbo run ci:test-a11y:chrome:atoms $LERNA_ARGS --concurrency=1",
    "ci:test-component:chrome:molecules": "cross-env-shell TEST_TYPE=component turbo run ci:test-component:chrome:molecules $LERNA_ARGS --concurrency=1",
    "ci:test:visual:molecules": "cross-env-shell TEST_TYPE=visual turbo run ci:test:visual:molecules $LERNA_ARGS --concurrency=1",
    "ci:test-a11y:chrome:molecules": "cross-env-shell TEST_TYPE=a11y turbo run ci:test-a11y:chrome:molecules $LERNA_ARGS --concurrency=1",
    "ci:test-component:chrome:organisms": "cross-env-shell TEST_TYPE=component turbo run ci:test-component:chrome:organisms $LERNA_ARGS --concurrency=1",
    "ci:test:visual:organisms": "cross-env-shell TEST_TYPE=visual turbo run ci:test:visual:organisms $LERNA_ARGS --concurrency=1",
    "ci:test-a11y:chrome:organisms": "cross-env-shell TEST_TYPE=a11y turbo run ci:test-a11y:chrome:organisms $LERNA_ARGS --concurrency=1",
    "ci:test-component:chrome:pages": "cross-env-shell TEST_TYPE=component turbo run ci:test-component:chrome:pages $LERNA_ARGS --concurrency=1",
    "ci:test:visual:pages": "cross-env-shell TEST_TYPE=visual turbo run ci:test:visual:pages $LERNA_ARGS --concurrency=1",
    "ci:test-a11y:chrome:pages": "cross-env-shell TEST_TYPE=a11y turbo run ci:test-a11y:chrome:pages $LERNA_ARGS --concurrency=1",
    "test-component:chrome": "cross-env-shell TEST_TYPE=component turbo run test-component:chrome $LERNA_ARGS --concurrency=1",
    "test:visual": "cross-env-shell TEST_TYPE=visual turbo run test:visual $LERNA_ARGS --concurrency=1",
    "test-a11y:chrome": "cross-env-shell TEST_TYPE=a11y turbo run test-a11y:chrome $LERNA_ARGS --concurrency=1",
    "test:wait-for-server": "node ./test/infrastructure/healthcheck.js",
    "allure:generate": "allure generate ./test/results/allure-results --clean && yarn allure:clean",
    "allure:open": "yarn allure:generate && yarn allure open",
    "allure:clean": "rimraf test/results/allure-results",
    "prepare": "husky install"
  },
  "resolutions": {
    "webpack": "4.46.0"
  },
  "devDependencies": {
    "@babel/cli": "7.14.8",
    "@babel/eslint-parser": "7.16.5",
